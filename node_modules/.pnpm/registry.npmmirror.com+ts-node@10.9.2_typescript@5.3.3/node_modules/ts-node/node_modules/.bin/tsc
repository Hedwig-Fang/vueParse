#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/bin/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules:/Users/hedwig/project/开源事业/node_modules:/Users/hedwig/project/node_modules:/Users/hedwig/node_modules:/Users/node_modules:/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.2_typescript@5.3.3/node_modules/ts-node/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.2_typescript@5.3.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/bin/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+typescript@5.3.3/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules:/Users/hedwig/project/开源事业/node_modules:/Users/hedwig/project/node_modules:/Users/hedwig/node_modules:/Users/node_modules:/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.2_typescript@5.3.3/node_modules/ts-node/node_modules:/Users/hedwig/project/开源事业/vueParce/node_modules/.pnpm/registry.npmmirror.com+ts-node@10.9.2_typescript@5.3.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+typescript@5.3.3/node_modules/typescript/bin/tsc" "$@"
fi
